<h1>Rotten Mangoes</h1>
<%= link_to "Create a new user!", new_user_path %>
<hr>
<% @users.each do |user| %>
  <h2><%= link_to user.full_name, admin_user_path(user) %></h2>
  <h2><%= user.email %></h2>
  <h3><%= formatted_date(user.created_at) %> <h3>
  <h3><%= formatted_date(user.updated_at) %> <h3>
  <h3>admin: <%= user.admin%></h3>
  <hr>
<% end %>

